{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "sources": "${sources_url}",
    "issues": "${issues_url}"
  },
  "license": "${license}",
  "icon": "${mod_id}_icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.illusivesoulworks.elytraslot.ElytraSlotFabricMod"
    ],
    "client": [
      "com.illusivesoulworks.elytraslot.ElytraSlotFabricClientMod"
    ]
  },
  "mixins": [
    "elytraslot-integrations.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14",
    "fabric": "*",
    "trinkets": "*",
    "minecraft": "${minecraft_version_range_alt}",
    "java": ">=17"
  }
}
